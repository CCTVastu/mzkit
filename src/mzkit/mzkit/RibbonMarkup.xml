	<Application.Views>
		<Ribbon>

			<Ribbon.ContextualTabs>
				<TabGroup CommandName='cmdTabGroupTableTools'>
					<Tab CommandName='cmdTabDesign'>
						<Group CommandName='cmdGroupDesign'>
							<Spinner CommandName="cmdSpinner" />
							<Button CommandName='cmdButtonDesign2' />
							<Button CommandName='cmdButtonDesign3' />
						</Group>
					</Tab>
					<Tab CommandName='cmdTabLayout'>
						<Group CommandName='cmdGroupLayout' SizeDefinition='TwoButtons'>
							<Button CommandName='cmdButtonLayout1' />
							<Button CommandName='cmdButtonLayout2' />
						</Group>
					</Tab>
				</TabGroup>


				<TabGroup CommandName='cmdTabGroupCalculatorTools'>
					<Tab CommandName='cmdTabCalculator'>
						<Group CommandName='cmdGroupCalculator' SizeDefinition='TwoButtons'>
							<Button CommandName='cmdButtonCalculatorExport' />
							<Button CommandName='cmdButtonLayout2' />
						</Group>
					</Tab>
				</TabGroup>


				
			</Ribbon.ContextualTabs>

			<Ribbon.HelpButton>
				<HelpButton CommandName="cmdHelpButton" />
			</Ribbon.HelpButton>


			<!-- Main Menu -->

			<Ribbon.Tabs>
				<Tab CommandName="cmdTabMain">
					<!-- scary part -->
					<Tab.ScalingPolicy>
						<ScalingPolicy>
							<ScalingPolicy.IdealSizes>
								<Scale Group="cmdGroupFileActions" Size="Large" />
								<Scale Group="cmdGroupExit" Size="Large" />
							</ScalingPolicy.IdealSizes>
							<Scale Group="cmdGroupFileActions" Size="Medium" />
						</ScalingPolicy>
					</Tab.ScalingPolicy>

					<!-- useful part -->
					<Group CommandName="cmdGroupFileActions" SizeDefinition="ThreeButtons">
						<Button CommandName="cmdButtonNew" />
						<Button CommandName="cmdButtonOpenRaw" />
						<Button CommandName="cmdButtonSave" />
					</Group>
					<Group CommandName="cmdGroupExit" SizeDefinition="OneButton">
						<Button CommandName="cmdButtonExit" />
					</Group>
				</Tab>


				<!-- Tools Menu -->

				<Tab CommandName="cmdTabTools">
					<!-- scary part -->
					<Tab.ScalingPolicy>
						<ScalingPolicy>
							<ScalingPolicy.IdealSizes>
								<Scale Group="cmdGroupToolsActions" Size="Large" />
							</ScalingPolicy.IdealSizes>
							<Scale Group="cmdGroupToolsActions" Size="Medium" />
						</ScalingPolicy>
					</Tab.ScalingPolicy>

					<!-- useful part -->
					<Group CommandName="cmdGroupToolsActions" SizeDefinition="ThreeButtons">
						<Button CommandName="cmdButtonMzCalculator" />
						<Button CommandName="cmdButtonMzSearch" />
						<Button CommandName="cmdButtonSave" />
					</Group>

				</Tab>


				<!-- About Tab -->

				<Tab CommandName="cmdTabAbout">
					<!-- scary part -->
					<Tab.ScalingPolicy>
						<ScalingPolicy>
							<ScalingPolicy.IdealSizes>
								<Scale Group="cmdGroupAboutActions" Size="Large" />
							</ScalingPolicy.IdealSizes>
							<Scale Group="cmdGroupAboutActions" Size="Medium" />
						</ScalingPolicy>
					</Tab.ScalingPolicy>

					<!-- useful part -->
					<Group CommandName="cmdGroupAboutActions" SizeDefinition="ThreeButtons">
						<Button CommandName="cmdButtonAbout" />
						<Button CommandName="cmdButtonOpenRaw" />
						<Button CommandName="cmdButtonSettings" />
					</Group>

				</Tab>

			</Ribbon.Tabs>


			<Ribbon.ApplicationMenu>
				<ApplicationMenu>

					<ApplicationMenu.RecentItems>
						<RecentItems CommandName="cmdRecentItems" EnablePinning="true" MaxCount="13" />
					</ApplicationMenu.RecentItems>

					<MenuGroup CommandName='cmdMenuGroupFile'>
						<Button CommandName='cmdButtonNew' />

						<DropDownButton CommandName='cmdButtonOpen'>
							<MenuGroup Class='MajorItems'>
								<Button CommandName='cmdButtonDropA' />
								<Button CommandName='cmdButtonDropB' />
								<Button CommandName='cmdButtonDropC' />
							</MenuGroup>
						</DropDownButton>

						<Button CommandName='cmdButtonSave' />
					</MenuGroup>
					<!--
					<MenuGroup>

						

						<SplitButton>
							<SplitButton.ButtonItem>
								<Button CommandName='cmdButtonDropB' />
							</SplitButton.ButtonItem>
							<SplitButton.MenuGroups>
								<MenuGroup Class='MajorItems'>
									<Button CommandName='cmdButtonDropA' />
									<Button CommandName='cmdButtonDropB' />
									<Button CommandName='cmdButtonDropC' />
								</MenuGroup>
							</SplitButton.MenuGroups>
						</SplitButton>

					</MenuGroup>-->
					<MenuGroup CommandName='cmdMenuGroupExit'>
						<Button CommandName='cmdButtonExit' />
					</MenuGroup>

				</ApplicationMenu>
			</Ribbon.ApplicationMenu>

			<Ribbon.QuickAccessToolbar>
				<QuickAccessToolbar CommandName='cmdQAT' CustomizeCommandName='cmdCustomizeQAT'>
					<QuickAccessToolbar.ApplicationDefaults>
						<Button CommandName="cmdButtonPageNavBack" ApplicationDefaults.IsChecked="true" />
						<Button CommandName="cmdButtonOpenRaw" ApplicationDefaults.IsChecked="true" />
						<Button CommandName="cmdButtonSave" ApplicationDefaults.IsChecked="false" />
					</QuickAccessToolbar.ApplicationDefaults>
				</QuickAccessToolbar>
			</Ribbon.QuickAccessToolbar>

		</Ribbon>
	</Application.Views>
</Application>
