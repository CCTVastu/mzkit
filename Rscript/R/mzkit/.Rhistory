exactMass = function(formula) {
if(class(formula) == "character") {
formula = parseFormula(formula);
}
atoms = atomics();
exact = 0;
for(atom in names(formula)) {
n = formula[[atom]];
a = atoms[[atom]];
exact = exact + a$isotopic * n;
}
exact;
}
exactMass()
exactMass("H2O")
exactMass("CH3")
exactMass("CH3OOH")
